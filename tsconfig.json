{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "target": "es6",
    "lib": [
      "es2017",
      "dom"
    ],
    "types": [
      "node",
      "@webgpu/types"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
